{
  "name": "jest-matcher-percent-error",
  "version": "1.1.2",
  "description": "A Jest matcher for testing percent error.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "check-types": "tsc -p tsconfig.check.json",
    "eslint": "eslint --ext .js,.ts src",
    "gen-declarations": "tsc -p tsconfig.declarations.json",
    "prepare": "run-p build gen-declarations",
    "test": "run-p check-types eslint test-only",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianprime0509/jest-matcher-percent-error.git"
  },
  "keywords": [
    "percent",
    "error",
    "jest",
    "matcher",
    "relative"
  ],
  "author": "Ian Johnson <ian@ianjohnson.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianprime0509/jest-matcher-percent-error/issues"
  },
  "homepage": "https://github.com/ianprime0509/jest-matcher-percent-error#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^4.4.3"
  }
}
